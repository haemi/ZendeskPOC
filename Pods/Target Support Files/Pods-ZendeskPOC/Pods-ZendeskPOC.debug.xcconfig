CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/ZDCChat" "${PODS_ROOT}/ZendeskSDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ZDCChat" "${PODS_ROOT}/Headers/Public/ZendeskSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ZDCChat" -isystem "${PODS_ROOT}/Headers/Public/ZendeskSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "MessageUI" -framework "MobileCoreServices" -framework "Security" -framework "SystemConfiguration" -framework "ZDCChat" -framework "ZendeskProviderSDK" -framework "ZendeskSDK"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
